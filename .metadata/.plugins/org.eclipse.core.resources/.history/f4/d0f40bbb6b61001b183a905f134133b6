package repeticao;

public class ExemploFor {

	public static void main(String[] args) {
		/*
		 * Sintaxe For:
		 * 1º - identifica o contador e qual o seu inicio
		 * 2º - condição (até onde deve repetir)
		 * 3º - Como será a contagem
		 */
		
		for (int contador=0;contador<500;contador++) {
			System.out.println(contador);
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
