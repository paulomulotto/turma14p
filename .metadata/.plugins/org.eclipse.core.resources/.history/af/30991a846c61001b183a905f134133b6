package repeticao;

import javax.swing.JOptionPane;

public class ExemploFor {

	public static void main(String[] args) {
		/*
		 * Sintaxe For:
		 * 1º - identifica o contador e qual o seu inicio
		 * 2º - condição (até onde deve repetir)
		 * 3º - Como será a contagem
		 */
		int tabuada = Integer.parseInt(JOptionPane.showInputDialog("Digite a tabuada"));
		for (int contador=1;contador<11;contador+=1) {
			System.out.println(contador*tabuada);
		}
		

		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
