		Pessoa candidato = new Pessoa();
		candidato = new Candidato("NOME CANDIDATO 2", 1, "PREFEITO", "Ã‰NOIX");
		
		Voto voto = new Voto(eleitor, candidato, true); 
		
		urna.addVotos(voto);
		
		
		

	}

}
